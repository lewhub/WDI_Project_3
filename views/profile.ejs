
<h4> <%#= user.local.name || user.facebook.name %> </h4>
  <div class="parallax-container">
     <div class="parallax"><img src="https://cdn.tutsplus.com/photo/uploads/legacy/335_panoramicimages/1.jpg"></div>
   </div>

   <div class="profile-space">
     <% if(user.local.name){ %>
         <h3>Locally Stored User</h3>

         <p>
             <strong>Name</strong>: <%= user.local.name %><br>
             <strong>id</strong>: <%= user._id %><br>
             <strong>email</strong>: <%= user.local.email %><br>
             <strong>password</strong>: <%= user.local.password %>
               <div class="row">
                 <div class="col s8">
                   <h5>Descriptions</h5>
                     <p>
                       <%= user.local.description %>
                     </p>
                </div>
              </div>
         </p>
         <% } else {%>
           <h3>Facebook User</h3>
           <p>
               <strong>Name</strong>: <%= user.facebook.name %><br>
               <strong>id</strong>: <%= user.facebook.id %><br>
               <strong>email</strong>: <%= user.facebook.email %><br>
               <strong>token</strong>: <%= user.facebook.token %>
               <div class="row">
                 <div class="col s8">
                   <h5>Descriptions</h5>
                     <p>
                       <%= user.facebook.description %>
                     </p>
                </div>
              </div>
           </p>
         <% } %>
